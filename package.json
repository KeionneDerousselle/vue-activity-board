{
  "name": "vue-activity-board",
  "version": "1.0.0",
  "description": "My top-notch Nuxt.js project",
  "author": "Keionne Derousselle",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=local nodemon server/index.js --watch server",
    "build": "npm run db:migrate && nuxt build",
    "start": "cross-env NODE_ENV=prod node server/index.js",
    "generate": "nuxt generate",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "heroku-run-build-script": true,
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "ant-design-vue": "^1.1.10",
    "cloudinary": "^1.13.2",
    "cloudinary-core": "^2.6.2",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "http-errors": "^1.7.1",
    "nuxt": "^2.4.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jest": ">=21.24.1",
    "eslint-plugin-node": ">=7.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^23.6.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.2"
  }
}
